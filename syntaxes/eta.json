{
	"name": "Eta",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"injectionSelector": "L:text.html.eta",
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control",
					"begin": "(<%[-_]?\\s*?(~|=)?)",
					"end": "([-_]?%>)",
					"contentName": "meta.embedded.block.javascript",
					"patterns": [
						{
							"include": "source.js"
						}
					]
				},
				{
					"name": "html.eta",
					"include": "text.html.derivative"
				}
			]
		}
	},
	"scopeName": "source.eta"
}